    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@types/uuid": "^10.0.0",
