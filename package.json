{
  "name": "captable-management-platform",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "description": "Enterprise-grade cap table management platform with financial-grade precision calculations, scenario modeling, and regulatory compliance features",
  "scripts": {
    "dev": "vite --config vite.config.optimized.ts",
    "dev:legacy": "vite",
    "build": "vite build --config vite.config.emergency.ts",
    "build:production": "vite build --config vite.config.emergency.ts",
    "build:emergency": "vite build --config vite.config.emergency.ts",
    "build:no-ts": "vite build --config vite.config.no-ts.js",
    "build:original": "vite build --config vite.config.optimized.ts",
    "build:analyze": "tsc && vite build --config vite.config.optimized.ts && open dist/bundle-analysis.html",
    "build:legacy": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "perf:analyze": "npm run build:analyze",
    "perf:lighthouse": "npx lighthouse http://localhost:3000 --view",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "keywords": [
    "cap-table",
    "equity-management",
    "financial-calculations",
    "startup-tools",
    "securities",
    "vesting",
    "409a-compliance",
    "scenario-modeling",
    "waterfall-analysis",
    "react",
    "typescript",
    "self-hosted"
  ],
  "author": {
    "name": "Cap Table Management Platform Contributors",
    "url": "https://github.com/your-org/captable"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/captable.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/captable/issues"
  },
  "homepage": "https://github.com/your-org/captable#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
